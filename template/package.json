{
    "name": "template",
    "description": "RMS plugins template",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --devtool inline-source-map --entry ./src/demo/index.js --open",
        "build:js": "webpack --mode production",
        "build:js-dev": "webpack --mode development",
        "build:js-demo": "webpack --mode production --entry ./src/demo/index.js",
        "build:py": "dash-generate-components ./src/lib/components webviz_subsurface_components -p package-info.json --r-prefix ''",
        "build": "npm run build:js && npm run build:js-dev && npm run build:py",
        "format": "eslint --fix *.js *json 'src/**/*.+(ts|tsx|js|jsx|json|css)'",
        "linting": "eslint *.js *.json 'src/**/*.+(ts|tsx|js|jsx|json|css)'"
    },
    "author": "R&T Equinor <hakal@equinor.com>",
    "license": "LGPL",
    "dependencies": {
        "@material-ui/core": "^4.3.3",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "bootstrap": "^4.5.0",
        "react-bootstrap": "^1.0.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.32.0",
        "@typescript-eslint/parser": "^2.32.0",
        "copy-webpack-plugin": "^6.0.1",
        "copyfiles": "^2.0.0",
        "css-loader": "^2.1.1",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "json-schema": "^0.2.5",
        "mini-css-extract-plugin": "^0.8.0",
        "npm": "^6.14.4",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prettier": "^1.18.2",
        "raw-loader": "^3.1.0",
        "react": "^16.9.0",
        "react-docgen": "^2.20.1",
        "react-dom": "^16.9.0",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^7.0.3",
        "typescript": "^3.8.3",
        "url-loader": "^2.1.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.11.0"
    },
    "engines": {
        "node": ">=12.14.0",
        "npm": ">=6.13.4"
    }
}
